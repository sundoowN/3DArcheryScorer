<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.Pages.ArcheryHomePage">
    <VerticalStackLayout
        Padding="10"
        Spacing="10">
        <Button Text="Scorecard Keeper"
                Clicked="OpenScoringApplication"></Button>
        <Button
            Text="Review Scores"
            Clicked="ReviewScores">
        </Button>
        <Button
            Text="Bow Setups"
            Clicked="BowSetups">            
        </Button>
        <Button
            Text="Arrow Setups"
            Clicked="ArrowSetups">
        </Button>
    </VerticalStackLayout>
</ContentPage>